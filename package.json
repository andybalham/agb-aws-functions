{
  "name": "@andybalham/agb-aws-functions",
  "version": "1.0.5",
  "description": "A set of base classes for creating AWS Lambda functions.",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "lint": "eslint ./src --ext .ts",
    "lint-and-fix": "eslint ./src --ext .ts --fix",
    "build": "npm run lint && tsc",
    "test": "mocha --require ts-node/register test/**/*.ts",
    "test-coverage": "nyc npm run test",
    "apigateway-synth": "tsc && cdk synth ApiGatewayFunctionTest",
    "apigateway-deploy": "tsc && cdk deploy ApiGatewayFunctionTest",
    "apigateway-test": "mocha --require ts-node/register test-deploy/test/**/ApiGateway*.ts",
    "sns-synth": "tsc && cdk synth SNSFunctionTest",
    "sns-deploy": "tsc && cdk deploy SNSFunctionTest",
    "sns-test": "mocha --require ts-node/register test-deploy/test/**/SNS*.ts",
    "sqs-synth": "tsc && cdk synth SQSFunctionTest",
    "sqs-deploy": "tsc && cdk deploy SQSFunctionTest",
    "sqs-test": "mocha --require ts-node/register test-deploy/test/**/SQS*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andybalham/agb-aws-functions.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/andybalham/agb-aws-functions/issues"
  },
  "homepage": "https://github.com/andybalham/agb-aws-functions#readme",
  "devDependencies": {
    "@andybalham/agb-aws-clients": "^1.0.6",
    "@aws-cdk/aws-apigateway": "1.91.0",
    "@aws-cdk/aws-lambda": "1.91.0",
    "@aws-cdk/aws-lambda-event-sources": "1.91.0",
    "@aws-cdk/aws-lambda-nodejs": "1.91.0",
    "@aws-cdk/aws-logs": "1.91.0",
    "@aws-cdk/aws-sns": "1.91.0",
    "@aws-cdk/aws-sns-subscriptions": "1.91.0",
    "@aws-cdk/aws-stepfunctions": "1.91.0",
    "@aws-cdk/aws-stepfunctions-tasks": "1.91.0",
    "@aws-cdk/core": "1.91.0",
    "@dazn/lambda-powertools-correlation-ids": "^1.24.1",
    "@dazn/lambda-powertools-logger": "^1.24.1",
    "@dazn/lambda-powertools-middleware-correlation-ids": "^1.24.0",
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@middy/core": "^1.5.2",
    "@middy/http-error-handler": "^1.5.2",
    "@middy/sqs-partial-batch-failure": "^1.5.2",
    "@types/aws-lambda": "^8.10.64",
    "@types/chai": "^4.2.11",
    "@types/http-errors": "^1.8.0",
    "@types/mocha": "^8.2.1",
    "@types/node": "^14.14.31",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "aws-sdk": "^2.853.0",
    "aws-sdk-mock": "^5.1.0",
    "axios": "^0.21.1",
    "chai": "^4.2.0",
    "dotenv": "^8.2.0",
    "esbuild": "^0.9.0",
    "eslint": "^7.20.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-standard": "^5.0.0",
    "mocha": "^8.3.0",
    "nyc": "^15.1.0",
    "prettier": "^2.2.1",
    "source-map-support": "^0.5.19",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.2"
  },
  "dependencies": {
    "http-errors": "^1.8.0",
    "nanoid": "^3.1.20"
  }
}
